{
  "name": "Imitate Image",
  "version": "0.5.1",
  "manifest_version": 2,
  "permissions": ["contextMenus", "clipboardWrite", "notifications", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "applications": {
    "gecko": {
      "id": "imitate-image@suienzan"
    }
  }
}
